{
  "name": "typescript-result-option",
  "version": "0.2.0",
  "description": "Implementation of Rust's Result and Option in TypeScript.",
  "private": false,
  "repository": "https://github.com/trvswgnr/typescript-result-option",
  "scripts": {
    "build": "tsx scripts/build.ts",
    "ci": "turbo build check-exports lint lint-pkg-json test",
    "check-exports": "check-export-map",
    "lint": "tsc",
    "lint-pkg-json": "tsx scripts/lint.ts",
    "release": "turbo run publish",
    "publish": "changeset publish",
    "test": "jest"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./index": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./util": {
      "types": "./dist/util.d.ts",
      "import": "./dist/util.mjs",
      "default": "./dist/util.js"
    },
    "./optional": {
      "types": "./dist/optional.d.ts",
      "import": "./dist/optional.mjs",
      "default": "./dist/optional.js"
    },
    "./result": {
      "types": "./dist/result.d.ts",
      "import": "./dist/result.mjs",
      "default": "./dist/result.js"
    }
  },
  "keywords": ["result", "option", "rust", "typescript", "ts", "js", "javascript"],
  "author": "Travis Aaron Wagner",
  "license": "MIT",
  "devDependencies": {
    "@types/eslint": "^8.21.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.4",
    "@changesets/cli": "^2.26.0",
    "@types/node": "^18.14.0",
    "@types/jest": "^29.4.0",
    "jest": "^29.4.3",
    "check-export-map": "^1.3.0",
    "tsx": "^3.12.3",
    "turbo": "^1.8.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
